#include <stdio.h>

int main() {
    int n, i;

    printf("Enter number of blocks in file: ");
    scanf("%d", &n);

    int blocks[n];

    printf("Enter block numbers:\n");
    for(i = 0; i < n; i++)
        scanf("%d", &blocks[i]);

    printf("\nLinked Allocation:\n");
    for(i = 0; i < n-1; i++)
        printf("%d -> ", blocks[i]);

    printf("%d -> NULL\n", blocks[n-1]);

    return 0;
}
